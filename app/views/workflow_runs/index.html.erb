

<p id="notice"><%= notice %></p>



<div class="container">

<nav class="navbar navbar-expand-lg navbar-light bg-light">
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item active">
        <a class="nav-link" href="#">Workflow Runs (Actions) </a>
      </li>
      <li class="nav-item">
        <a class="nav-link" href="#">Packages</a>
      </li>
    </ul>
  </div>
</nav>

    <div class="row">
      <h1 class="col-md-10">Workflow Runs</h1>
      <h1 class="float-right col-md-2"><%= link_to "Download .xlsx file" %></h1>
    </div>


  <table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Minutes</th>
        <th scope="col">Workflow Name</th>
        <th scope="col">Sender</th>
        <th scope="col">Repo</th>
        <th scope="col">Repo-Owner</th>
        <th scope="col">Workflow ID</th>
        <th scope="col">Billing Cycle</th>
        <th colspan="4"></th>
      </tr>
    </thead>

    <tbody>
      <% @workflow_runs.each do |workflow_run| %>
        <tr>
          <td><%= workflow_run.minutes %></td>
          <td><%= workflow_run.workflow_name %></td>
          <td><%= workflow_run.sender %></td>
          <td><%= workflow_run.repo %></td>
          <td><%= workflow_run.repo_owner %></td>
          <td><%= workflow_run.workflow_id %></td>
          <td><%= workflow_run.billing_cycle %></td>
          <td><%= link_to 'Show', workflow_run %></td>

        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<br>

